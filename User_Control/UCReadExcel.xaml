<UserControl x:Class="ServerDTT_New_.ExtendedWindow.UCReadExcel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ServerDTT_New_.ExtendedWindow"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" Background="DarkBlue">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>

        <Button Grid.Row="0" Name="btnLoadQuestion" Content="Offical" Click="BtnLoadQuestion_Click"/>
        <Button Grid.Row="1" Name="btnLoadBUQuestion" Content="Backup" Click="BtnLoadBUQuestion_Click"/>
        <TextBox Grid.Row="4" Name="txtBlockFinish" />
        <TextBox Grid.Row="2" x:Name="txtMatch"/>
        <TextBox Grid.Row="3" x:Name="txtName"/>

    </Grid>
</UserControl>
